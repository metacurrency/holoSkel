#!/bin/sh
sleep 2

echo #!/bin/sh $0
seedID=`cat /chain.seeded/devchain/.seedID`
clusterDir="/chain.seeded/devchain/cluster/$seedID"

echo "ls $clusterDir/hc"
ls $clusterDir/hc



counter=1
for container in `ls $clusterDir/hc`; do
	cp /Scripts/cluster/hc/$counter.* $clusterDir/hc/$container
	counter=$(( counter+1 ))
done



echo "unlocking cluster $seedID"
rm $clusterDir/.cluster.lock


